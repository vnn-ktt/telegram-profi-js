{
  "name": "telegram-profi",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "clean": "rimraf distribution",
    "clean:dev": "rimraf distribution/development",
    "clean:prod": "rimraf distribution/product",
    "build:dev": "yarn run clean:dev && cross-env NODE_ENV=development tsc -p tsconfig.dev.json",
    "build:prod": "yarn run clean:prod && cross-env NODE_ENV=product tsc -p tsconfig.prod.json",
    "start:dev": "nodemon distribution/development/index.js",
    "start:prod": "nodemon distribution/product/index.js",
    "develop": "yarn run build:dev && yarn run start:dev",
    "deploy": "yarn run build:prod && yarn run start:prod",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "@eslint/js": "^9.36.0",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.2",
    "fs": "^0.0.1-security",
    "globals": "^16.4.0",
    "grammy": "^1.38.2",
    "mongodb": "^6.20.0",
    "mongoose": "^8.18.1",
    "nodemon": "^3.1.10",
    "path": "^0.12.7",
    "random-js": "^2.1.0",
    "tsconfig": "^7.0.0",
    "typescript": "^5.9.2",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@types/mongodb": "^4.0.7",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^24.3.1",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "cross-env": "^10.0.0",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1"
  }
}
